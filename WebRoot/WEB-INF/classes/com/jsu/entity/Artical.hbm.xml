<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC

        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"

        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
 <class name="com.jsu.entity.Artical" table="tb_artical">
 <id name="id" type="string" column="artical_id"></id>
 <property name="time" type="string" column="artical_time"/>
 <property name="title" type="string" column="artical_title"/>
 <property name="img" type="string" column="artical_img"/>
 <property name="details" type="string" column="artical_details"/>
  </class>

</hibernate-mapping>